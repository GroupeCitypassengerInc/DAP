
config uhttpd 'main'
	option redirect_https '1'
	option home '/www'
	option rfc1918_filter '1'
	option max_requests '3'
	option max_connections '100'
	option cgi_prefix '/cgi-bin'
	option script_timeout '60'
	option network_timeout '30'
	option http_keepalive '20'
	option tcp_keepalive '1'
	option ubus_prefix '/ubus'
	option listen_http '172.18.9.2:80'

config uhttpd 'portal'
	option listen_http '10.168.168.1:8081'
	option home '/portal'
	option lua_prefix '/ws/wifi'
	option lua_handler '/portal/index.lua'
	option script_timeout '60'
	option network_timeout '30'
	option http_keepalive '20'
	option tcp_keepalive '1'
	option max_requests '3'

config uhttpd 'proxy'
	option listen_http '10.168.168.1:80'
	option home '/portal'
	option lua_prefix '/'
	option lua_handler '/portal/portal.lua'
	option script_timeout '60'
	option network_timeout '30'
	option http_keepalive '20'
	option tcp_keepalive '1'
	option max_requests '3'

config cert 'defaults'
	option days '730'
	option bits '2048'
	option country 'ZZ'
	option state 'Somewhere'
	option location 'Unknown'
	option commonname 'LEDE'

